{
  "name": "dce-student-portal",
  "version": "1.0.0",
  "description": "mern app",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "installBackend": "npm install express bcryptjs body-parser cloudinary cookie-parser dotenv express-fileupload jsonwebtoken mongoose nodemailer stripe validator colors express-async-handler nodemon mongodb concurrently axios",
    "installClient": "npm install --prefix frontend",
    "installPython": "pip install flask roboflow --prefix python",
    "install": "concurrently \"npm run installBackend\" \"npm run installClient\" ",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server \" \"npm run client\" ",
    "ngrok": "ngrok http http://localhost:3000 --host-header=\"localhost:300\"",
    "liveProduction": "concurrently \"npm run server \" \"npm run client\" \"npm run ngrok\" ",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d"
  },
  "author": "Mukul Dahiya",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cloudinary": "^1.41.3",
    "colors": "^1.4.0",
    "concurrently": "^7.6.0",
    "cookie": "^0.6.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-async-handler": "^1.2.0",
    "express-fileupload": "^1.5.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^5.9.2",
    "mongoose": "^6.13.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.15",
    "openai": "^4.0.0",
    "path": "^0.12.7",
    "stripe": "^11.18.0",
    "universal-cookie": "^6.1.1",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "axios": "^1.7.7",
    "eslint": "^9.1.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "nodemon": "^3.1.7"
  }
}